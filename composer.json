{
    "name":"samsonos/php_log",
    "description": "SamsonPHP log module",
    "keywords": [ "error", "log", "debug", "samsonphp"],
    "authors":[
        {
            "name":"Vitaly Iegorov",
            "email":"egorov@samsonos.com"
        }
    ],
    "license":"Open Software License (OSL) v 3.0",
    "homepage":"http://samsonphp.com/",
    "autoload" : {
        "classmap": ["src/"],
        "psr-0": {
            "samsonos\\log\\": "src/",
            "samson\\log\\": "src/"
        },
        "files": ["src/global.php"]
    },	
    "require": {
    	"psr/log": "1.0.*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "phpunit/phpunit": "4.2.*"
    }
}
